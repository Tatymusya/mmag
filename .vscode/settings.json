{
  "eslint.validate": [
    "javascript",
    "html",
    "vue"
  ],
  "stylelint.enable": true,
  "css.validate": false,
  "scss.validate": false,
  "editor.codeActionsOnSave": { 
    "source.fixAll": true, 
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  }, 
  "editor.formatOnSave": true, 
  "javascript.format.enable": false, 
  "vetur.validation.template": false
}